          <boolProp name="resetInterpreter">false</boolProp>
          <stringProp name="script">import java.time.*;
import java.time.temporal.ChronoUnit;

if( ${StartAtTime} ){
	LocalTime t1 = LocalTime.now();
	log.info(&quot; &gt;&gt;&gt;&gt;&gt; &gt;&gt;&gt;&gt;&gt; &quot; + t1);
	LocalTime t2 = LocalTime.parse(&quot;${StartTime}&quot;);
	log.info(&quot; &gt;&gt;&gt;&gt;&gt; &gt;&gt;&gt;&gt;&gt; &quot; + t2);

	long t3 = ChronoUnit.MILLIS.between(t1, t2);
	log.info(&quot; &gt;&gt;&gt;&gt;&gt; &gt;&gt;&gt;&gt;&gt; &quot; + t3);

	if(t3 &gt; 0){
		return t3;
	}
	else{
		return 0;
	}
}
else{
	return 0;
}
</stringProp>
        </BeanShellTimer>
