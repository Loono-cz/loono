        <BeanShellSampler guiclass="BeanShellSamplerGui" testclass="BeanShellSampler" testname="Dynamic user profile variables" enabled="true">
          <stringProp name="BeanShellSampler.query">import java.util.Random;

//log.info(&quot; &gt;&gt;&gt;&gt;&gt; Print to log :)&quot;);

// Generate random SEX for OnBoarding
String[] list = {&quot;MALE&quot;,&quot;FEMALE&quot;};
Random rand = new Random();
vars.put(&quot;sex&quot;,list[rand.nextInt(2)]);

// Create Nick and prepare email for OnBoarding
String mail = vars.get(&quot;AccountMailPrefix&quot;)+vars.get(&quot;index&quot;)+&quot;@loono.cz&quot;; 
String nick = mail.split(&quot;@&quot;,2)[0];
vars.put(&quot;nick&quot;,nick);
vars.put(&quot;email&quot;, mail);

ResponseCode = &quot;200&quot;;
ResponseMessage = &quot;Dynamic data was generated&quot;;
IsSuccess = true;</stringProp>
